<?xml version="1.0" encoding="UTF-8"?>
<resources>

    <declare-styleable name="preferenceItem">
        <attr name="title" />
        <attr name="value" format="string" />
        <attr name="showArrow" format="boolean" />
        <attr name="showLine" format="boolean" />
        <attr name="textColor" format="color" />
        <attr name="p_icon" format="reference" />
        <attr name="p_icon_right" format="dimension" />
        <attr name="textSize" format="dimension" />
        <attr name="max" format="integer" />
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>


    <declare-styleable name="TabLayout">
        <attr name="tabIndicatorMargin" format="dimension"></attr>
        <attr name="tabIndicatorRadius" format="dimension"></attr>
    </declare-styleable>

    <attr name="overflowIcon" format="reference"/>
</resources>